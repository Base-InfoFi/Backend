{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "dist-seed",
    "rootDir": "./",
    "strict": false,

    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,

    "noEmitOnError": false
  },
  "include": [
    "scripts/**/*",
    "src/**/*",
    "prisma/**/*"
  ],
  "exclude": [
    "node_modules",
    ".next"
  ]
}
